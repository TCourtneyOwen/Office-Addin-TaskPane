{
  "name": "office-addin-taskpane",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "excel",
    "app-type-to-debug": "desktop"
  },
  "scripts": {
    "build": "rimraf dist && concurrently \"npm run tsc\" \"npm run copy\"",
    "copy": "cpx \"src/taskpane/taskpane.css\" dist && cpx \"assets/**/*.*\" dist/assets && cpx \"src/html/**/*.*\" dist",
    "convert-to-single-host": "node convertToSingleHost.js",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "sideload": "office-addin-debugging start manifest.xml",
    "start": "concurrently \"npm run start-server\" \"npm run sideload\" ",
    "start-server": "node server.js",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "tsc": "tsc -p tsconfig.json",
    "test": "mocha -r ts-node/register test/*.ts",
    "validate": "office-addin-manifest validate manifest.xml"
  },
  "dependencies": {
    "source-map": "^0.7.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.4",
    "@babel/core": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@types/express": "4.16.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@types/office-js": "^1.0.37",
    "@types/office-runtime": "^1.0.8",
    "concurrently": "^4.1.2",
    "cors": "2.8.5",
    "cpx": "1.5.0",
    "current-processes": "^0.2.1",
    "eslint-config-office-addins": "^1.0.10",
    "express": "^4.17.1",
    "mocha": "^6.2.0",
    "office-addin-cli": "^1.0.3",
    "office-addin-debugging": "^3.0.17",
    "office-addin-dev-certs": "^1.4.10",
    "office-addin-lint": "^1.0.13",
    "office-addin-prettier-config": "^1.0.10",
    "office-addin-test-helpers": "^1.0.13",
    "office-addin-test-server": "^1.0.16",
    "rimraf": "2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "prettier": "office-addin-prettier-config"
}
